{
  "name": "gymcore",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "pnpm --parallel --stream -r run dev",
    "dev:frontend": "pnpm --filter web-dashboard run dev",
    "dev:auth": "pnpm --filter auth-service run start:dev",
    "dev:payments": "pnpm --filter payment-service run start:dev",
    "dev:gyms": "pnpm --filter gym-management-service run start:dev",
    "db:migrate": "pnpm exec prisma migrate dev --schema=./backend/libs/database/prisma/schema.prisma",
    "db:generate": "pnpm exec prisma generate --schema=./backend/libs/database/prisma/schema.prisma"
  },
